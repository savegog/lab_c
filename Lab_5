#include <stdio.h>
#icnlude "function.h"
int main()
{
    int n;
    printf("Enter size matrix:");
    scanf("%d", &n);
    double* matrix1 = (double*)malloc(n*n*sizeof(double));
    double* matrix2 = (double*)malloc(n*n*sizeof(double));
    
    printf("Enter matrix1")
    for (int i; i<n;i++){
        for (int j; j<n; j++){
            scanf("%lf", &matrix1[i][j]);
        }
    }
    
    printf("Enter matrix2")
    for (int i; i<n;i++){
        for (int j; j<n; j++){
            scanf("%lf", &matrix2[i][j]);
        }
    }
    char operation;
    printf("Enter operation (+,-,*):");
    scanf("%c", &operation);
    double* result = matrix_op(double *matrix1, double *matrix2,char operation, int n);
    for (i = 0; i < n; i++) {
        for (j = 0; j < n; j++) {
            printf("%lf ", result[i][j]);
        }
    printf("\n";)
    }
     free(matrix1);
    free(matrix2);
    free(result);

    return 0;
}
